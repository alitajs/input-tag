{
  "private": true,
  "name": "@alitajs/input-tag",
  "version": "1.0.0",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "publish": "npm publish --access public",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.10.1",
    "antd": "^4.19.1",
    "classnames": "2.x",
    "rc-motion": "^2.0.1",
    "rc-overflow": "^1.0.0",
    "rc-trigger": "^5.0.4",
    "rc-util": "^5.19.2",
    "rc-virtual-list": "^3.2.0",
    "react": "^16.12.0 || ^17.0.0"
  },
  "devDependencies": {
    "@alita/test": "^0.0.4",
    "@types/enzyme": "^3.10.9",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.3",
    "cross-env": "^7.0.0",
    "dumi": "^1.1.32",
    "enzyme": "^3.3.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^7.1.0",
    "father": "^2.13.2",
    "jsonp": "^0.2.1",
    "np": "^7.5.0",
    "jest": "^27.1.0",
    "rc-dialog": "^8.1.1",
    "typescript": "^4.2.3",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@umijs/fabric": "^2.8.1",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "yorkie": "^2.0.0"
  }
}
